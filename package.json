{
	"name": "node-sht20",
	"description": "Library to work with a SHT20 chip via i2c",
	"version": "1.0.3",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"repository": "https://github.com/KillerJulian/node-sht20",
	"homepage": "https://killerjulian.github.io/node-sht20",
	"author": "KillerJulian <info@killerjulian.de>",
	"license": "MIT",
	"publishConfig": {
		"provenance": true
	},
	"packageManager": "pnpm@9.15.2",
	"dependencies": {
		"i2c-bus": "5.2.3",
		"tslib": "2.8.1"
	},
	"devDependencies": {
		"@types/i2c-bus": "5.1.2",
		"@types/node": "^16.18.123",
		"eslint": "9.17.0",
		"eslint-config-prettier": "9.1.0",
		"prettier": "3.4.2",
		"typedoc": "~0.27.6",
		"typedoc-github-theme": "^0.2.0",
		"typescript": "5.7.2",
		"typescript-eslint": "8.19.0"
	},
	"scripts": {
		"build": "pnpm build:lib && pnpm build:docs",
		"build:lib": "tsc",
		"build:docs": "typedoc src --plugin typedoc-github-theme",
		"lint": "pnpm lint:format && pnpm lint:code",
		"lint:format": "prettier --check .",
		"lint:code": "eslint ."
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"keywords": [
		"sht20"
	]
}
